<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<BorderPane fx:id="main_pane" prefHeight="583.0" prefWidth="636.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainController">
   <center>
      <BorderPane prefHeight="374.0" prefWidth="692.0" BorderPane.alignment="CENTER">
         <center>
              <TabPane fx:id="tap_pane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                  <tabs>
                      <fx:include source="TabPane.fxml" />
                  </tabs>
              </TabPane>
         </center>
         <top>
              <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                  <items>
                      <Button id="next_difference_button" fx:id="next_difference_button" mnemonicParsing="false" onAction="#clickNextDifferenceButton" prefHeight="32.0" prefWidth="32.0" text="Next Dif" />
                      <Button id="post_difference_button" fx:id="post_difference_button" mnemonicParsing="false" onAction="#clickPostDifferenceButton" prefHeight="32.0" prefWidth="32.0" text="Post Dif" />
                      <Button id="first_difference_button" fx:id="first_difference_button" mnemonicParsing="false" onAction="#clickFirstDifferenceButton" prefHeight="32.0" prefWidth="32.0" text="First Dif" />
                      <Button id="now_difference_button" fx:id="now_difference_button" mnemonicParsing="false" onAction="#clickNowDifferenceButton" prefHeight="32.0" prefWidth="32.0" text="Now Dif" />
                      <Button id="last_difference_button" fx:id="last_difference_button" mnemonicParsing="false" onAction="#clickLastDifferenceButton" prefHeight="32.0" prefWidth="32.0" text="Last Dif" />
                      <Button id="copy_to_right_button" fx:id="copy_to_right_button" mnemonicParsing="false" onAction="#clickCopyToRightButton" prefHeight="32.0" prefWidth="32.0" text="CtoR" />
                      <Button id="copy_to_left_button" fx:id="copy_to_left_button" mnemonicParsing="false" onAction="#clickCopyToLeftButton" prefHeight="32.0" prefWidth="32.0" text="CtoL" textAlignment="CENTER" />
                      <Button id="copy_to_right_all_button" fx:id="copy_to_right_all_button" mnemonicParsing="false" onAction="#clickCopyToRightAllButton" prefHeight="32.0" prefWidth="32.0" text="CtoRAll" />
                      <Button id="copy_to_left_all_button" fx:id="copy_to_left_all_button" mnemonicParsing="false" onAction="#clickCopyToLeftAllButton" prefHeight="32.0" prefWidth="32.0" text="CtoLAll" />
                      <Button id="compare_button" fx:id="compare_button" mnemonicParsing="false" onAction="#clickCompareButton" prefHeight="32.0" prefWidth="32.0" text="Compare" />
                  </items>
              </ToolBar>
         </top>
      </BorderPane>
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                <MenuItem fx:id="new_tab_menu_item" mnemonicParsing="false" onAction="#clickNewTabMenuItem" text="New tab" />
                <MenuItem fx:id="open_menu_item" mnemonicParsing="false" onAction="#clickOpenMenuItem" text="Open" />
                <MenuItem fx:id="save_menu_item" mnemonicParsing="false" onAction="#clickSaveMenuItem" text="Save" />
                <MenuItem fx:id="save_right_file_menu_item" mnemonicParsing="false" onAction="#clickSaveRightFileMenuItem" text="Save Right File" />
                <MenuItem fx:id="save_left_file_menu_item" mnemonicParsing="false" onAction="#clickSaveLeftFileMenuItem" text="Save Left File" />
                <MenuItem fx:id="close_menu_item" mnemonicParsing="false" onAction="#clickCloseMenuItem" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Merge">
            <items>
              <MenuItem fx:id="next_difference_menu_item" mnemonicParsing="false" onAction="#clickNextDifferenceMenuItem" text="Next Difference" />
                  <MenuItem fx:id="post_difference_menu_item" mnemonicParsing="false" onAction="#clickPostDifferenceMenuItem" text="Post Difference" />
                  <MenuItem fx:id="first_difference_menu_item" mnemonicParsing="false" onAction="#clickFirstDifferenceMenuItem" text="First Difference" />
                  <MenuItem fx:id="now_difference_menu_item" mnemonicParsing="false" onAction="#clickNowDifferenceMenuItem" text="Now Selected Difference" />
                  <MenuItem fx:id="last_difference_menu_item" mnemonicParsing="false" onAction="#clickLastDifferenceMenuItem" text="Last Difference" />
                  <MenuItem fx:id="copy_to_right_menu_item" mnemonicParsing="false" onAction="#clickCopyToRightMenuItem" text="Copy to Right" />
                  <MenuItem fx:id="copy_to_left_menu_item" mnemonicParsing="false" onAction="#clickCopyToLeftMenuItem" text="Copy to Left" />
                  <MenuItem fx:id="copy_to_right_all_menu_item" mnemonicParsing="false" onAction="#clickCopyToRightAllMenuItem" text="Copy to Right All" />
                  <MenuItem fx:id="copy_to_left_all_menu_item" mnemonicParsing="false" onAction="#clickCopyToLeftAllMenuItem" text="Copy to Left All" />
                  <MenuItem fx:id="compare_menu_item" mnemonicParsing="false" onAction="#clickCompareMenuItem" text="Compare" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
                <MenuItem fx:id="help_menu_item" mnemonicParsing="false" onAction="#clickHelpMenuItem" text="Help" />
                <MenuItem fx:id="program_information_menu_item" mnemonicParsing="false" onAction="#clickProgramInformationMenuItem" text="Program Info" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Window">
              <items>
                  <MenuItem fx:id="close_tab_menu_item" mnemonicParsing="false" onAction="#clickCloseTabMenuItem" text="Close Tab" />
                  <MenuItem fx:id="close_tab_all_menu_item" mnemonicParsing="false" onAction="#clickCloseTabAllMenuItem" text="Close Tab All" />
                  <MenuItem fx:id="tab_1_menu_item" mnemonicParsing="false" onAction="#clickTab1MenuItem" text="Tab 1" />
              </items>
            </Menu>
        </menus>
         <contextMenu>
            <ContextMenu />
         </contextMenu>
      </MenuBar>
   </top>
</BorderPane>
